<a href="/songs">Dashboard</a>
<h3>Other who added <%= @song.artist %> - <%= @song.title %></h3>
<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Times Added</th>
		</tr>
	</thead>
	<tbody>
		<% @song.users_added.uniq.each do |user| %>
		<tr>
			<td>
					<a href="/users/<%= user.id %>">
						<%= user.first_name %> <%= user.last_name %>
					</a>
			</td>

			<td><%= @playlist.count %></td>
		</tr>
		<% end %>
	</tbody>
</table>
